INCLUDE_DIRECTORIES(
    ${CMAKE_CURRENT_BINARY_DIR}
	${QT_INCLUDE_DIR}
	${QT_QT_INCLUDE_DIR}
	${QT_QTCORE_INCLUDE_DIR}
	${QT_LIBRARIES}
)

qt4_automoc(
	SonarInterface.cpp
	SonarScan.cpp
)

QT4_WRAP_CPP(sonarlib_MOC_SRCS 
	SonarInterface.h
	SonarScan.h
)


add_library(sonarlib SHARED
	SonarInterface.cpp
	SonarScan.cpp
	${tools_MOC_SRCS}
)
